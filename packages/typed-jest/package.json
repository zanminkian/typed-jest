{
  "name": "typed-jest",
  "version": "0.1.3",
  "description": "Test framework supporting TypeScript out-of-box",
  "keywords": [
    "testing",
    "typescript",
    "jest",
    "out-of-box",
    "ts-jest",
    "test",
    "framework"
  ],
  "homepage": "https://github.com/zanminkian/typed-jest",
  "repository": {
    "type": "git",
    "url": "https://github.com/zanminkian/typed-jest"
  },
  "license": "MIT",
  "author": "hellozmj@qq.com",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./supertest": {
      "require": "./dist/supertest.cjs",
      "import": "./dist/supertest.js"
    }
  },
  "types": "./src/index.d.ts",
  "bin": {
    "jest": "./src/bin/jest.js",
    "typed-jest": "./src/bin/typed-jest.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/supertest.ts --format cjs,esm --dts"
  },
  "dependencies": {
    "@jest/globals": "29.7.0",
    "@types/supertest": "6.0.2",
    "jest": "29.7.0",
    "lilconfig": "3.1.1",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/yargs": "17.0.32",
    "tsup": "8.0.2",
    "typescript": "5.4.4"
  }
}
